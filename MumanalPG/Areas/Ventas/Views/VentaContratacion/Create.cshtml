@model MumanalPG.Models.Ventas.VentaContratacion

@{
    ViewData["Title"] = "Prestaciones / ASR / Contratacion";
}

<h2>Nueva Contratación</h2>

@*<h4>Nueva Contratación</h4>*@
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <table>
                <tr>
                    <td> <label asp-for="IdUnidadEjecutora" class="control-label"></label> </td>
                    <td>
                        <input asp-for="IdUnidadEjecutora" class="form-control" />
                        <span asp-validation-for="IdUnidadEjecutora" class="text-danger"></span>
                    </td>
                </tr>
                <tr>
                    <td> <label asp-for="CorrelativoUnidad" class="control-label"></label> </td>
                    <td>
                        <input asp-for="CorrelativoUnidad" class="form-control" />
                        <span asp-validation-for="CorrelativoUnidad" class="text-danger"></span>
                    </td>
                </tr>
                <tr>
                    <td> <label asp-for="FechaVenta" class="control-label"></label> </td>
                    <td>
                        <input asp-for="FechaVenta" class="form-control" />
                        <span asp-validation-for="FechaVenta" class="text-danger"></span>
                    </td>
                </tr>
                <tr>
                    <td> <label asp-for="IdBeneficiario" class="control-label"></label> </td>
                    <td>
                        <input asp-for="IdBeneficiario" class="form-control" />
                        <span asp-validation-for="IdBeneficiario" class="text-danger"></span>
                    </td>
                </tr>
                <tr>
                    <td> <label asp-for="IdBeneficiarioGarante" class="control-label"></label> </td>
                    <td>
                        <input asp-for="IdBeneficiarioGarante" class="form-control" />
                        <span asp-validation-for="IdBeneficiarioGarante" class="text-danger"></span>
                    </td>
                </tr>
                <tr>
                    <td> <label asp-for="IdAsrSiver" class="control-label"></label> </td>
                    <td>
                        <input asp-for="IdAsrSiver" class="form-control" />
                        <span asp-validation-for="IdAsrSiver" class="text-danger"></span>
                    </td>
                </tr>
            </table>
            <br>
            <div class="form-group">
                <input type="submit" value="Grabar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
