@model IEnumerable<MumanalPG.Models.FinanzasParam.Partida>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdSubGrupo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gestion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VidaUtil)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdSubGrupo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gestion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VidaUtil)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPartida">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPartida">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPartida">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
